<Application x:Class="SolidStandAloneApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/AppMainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/TextDictionary.xaml"/>
                <ResourceDictionary Source="Resources/VectorsDictionary.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Support;component/GenericDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
